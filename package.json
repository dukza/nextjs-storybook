    "@storybook/react": "^6.3.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-named-asset-import": "^0.3.7",
    "gh-pages": "^3.2.3",
    "storybook-addon-designs": "^6.0.0"
  },
  "homepage": "https://dukza.github.io/nextjs-storybook/"
